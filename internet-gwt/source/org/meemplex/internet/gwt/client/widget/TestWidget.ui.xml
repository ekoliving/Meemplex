<ui:UiBinder 
	xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:gwt="urn:import:org.meemplex.internet.gwt.client.widget"
>
	<g:HTMLPanel>
		<p ui:field="number" class="number">11</p>
		<h3 ui:field="symbol" class="symbol">
			<g:ToggleButton ui:field="toggle" styleName="testToggle">
				<g:upFace>up</g:upFace>
				<g:upHoveringFace>uph</g:upHoveringFace>
				<g:upDisabledFace>upd</g:upDisabledFace>
				<g:downFace>df</g:downFace>
				<g:downHoveringFace>dh</g:downHoveringFace>
				<g:downDisabledFace>dd</g:downDisabledFace>
			</g:ToggleButton>
		</h3>
		<h2 ui:field="name" class="name">name</h2>
		<p ui:field="weight" class="weight">40.074</p>
	</g:HTMLPanel>
</ui:UiBinder>
